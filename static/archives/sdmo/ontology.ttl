#################################################################

###  http://schema.org/contentUrl
<http://schema.org/contentUrl> rdf:type owl:DatatypeProperty ;
                               rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                               rdfs:comment "Actual bytes of the media object, for example the image file or video file."@en ;
                               rdfs:seeAlso "http://schema.org/contentUrl"^^xsd:anyURI .


###  http://schema.org/duration
<http://schema.org/duration> rdf:type owl:DatatypeProperty ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Program>
                                                         <http://sdm.hongo.wide.ad.jp/sdmo/SDMEvent>
                                                       )
                                         ] ;
                             rdfs:comment "The duration of the item (movie, audio recording, event, etc.) in ISO 8601 date format."@en ;
                             rdfs:seeAlso "http://schema.org/duration"^^xsd:anyURI .


###  http://schema.org/name
<http://schema.org/name> rdf:type owl:DatatypeProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                     <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                     <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                     <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                     <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                   )
                                     ] ;
                         rdfs:comment "The name of the item."@en ;
                         rdfs:seeAlso "http://schema.org/name"^^xsd:anyURI .


###  http://schema.org/startDate
<http://schema.org/startDate> rdf:type owl:DatatypeProperty ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Program>
                                                          <http://sdm.hongo.wide.ad.jp/sdmo/SDMEvent>
                                                        )
                                          ] ;
                              rdfs:comment "The start date and time of the item (in ISO 8601 date format)."@en ;
                              rdfs:seeAlso "http://schema.org/startDate"^^xsd:anyURI .


###  http://sdm.hongo.wide.ad.jp/sdmo/endAt
<http://sdm.hongo.wide.ad.jp/sdmo/endAt> rdf:type owl:DatatypeProperty ;
                                         rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                         rdfs:range xsd:dateTimeStamp ;
                                         rdfs:comment "The end time of the playback interval of the media. It defines the offset time to zero with the time specified by \"mediaStartAt.\""@en ,
                                                      "メディアの再生区間の終了時間。mediaStartAt で指定した時刻を０としたオフセット時間を指定する。"@ja ;
                                         rdfs:label "end at"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/eventTime
<http://sdm.hongo.wide.ad.jp/sdmo/eventTime> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent> ;
                                             rdfs:range xsd:dateTimeStamp ;
                                             rdfs:comment "Defines the time to start playing media in \"MediaEvent\" using the time axis of \"CompositeMedia.\" It represents that the playback section of the media starts at the specified time."@en ,
                                                          "MediaEventで   Media の再生開始時刻をCompositeMedia の時間軸で指定する。指定した時刻において Mediaの再生区間が始まることを表現する。"@ja ;
                                             rdfs:label "event time"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/mediaStartAt
<http://sdm.hongo.wide.ad.jp/sdmo/mediaStartAt> rdf:type owl:DatatypeProperty ;
                                                rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                                rdfs:range xsd:dateTimeStamp ;
                                                rdfs:comment "Notates the start time of the media file's valid section as an offset from its beginning. It defines time 0 when assigning the playback section with \"startAt\" and \"endAt.\""@en ,
                                                             "メディアファイルの有効区間開始時刻をメディアファイル先頭からのオフセットで表記する。startAt 及び endAt で再生区間を指定する際の時刻０を規定する。"@ja ;
                                                rdfs:label "media start at"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/startAt
<http://sdm.hongo.wide.ad.jp/sdmo/startAt> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                           rdfs:range xsd:dateTimeStamp ;
                                           rdfs:comment "The start time for the media playback section. It defines the offset time to 0 with the time specified by \"mediaStartAt.\""@en ,
                                                        "メディアの再生区間の開始時間。mediaStartAt で指定した時刻を０としたオフセット時間を指定する。"@ja ;
                                           rdfs:label "start at"@en .


#################################################################
#    Classes
#################################################################

###  http://data.ign.fr/def/geometrie#Point
<http://data.ign.fr/def/geometrie#Point> rdf:type owl:Class ;
                                         rdfs:comment "Geometric primitive to represent a geographical position. This position is represented via X and Y coordinates expressed in a given coordinate system. If the coordinate system associated with the point allows it, the point may also have Z and M coordinates."@en ;
                                         rdfs:isDefinedBy "<http://data.ign.fr/def/geometrie>" .


###  http://data.ign.fr/def/ignf#CoordinateSystem
<http://data.ign.fr/def/ignf#CoordinateSystem> rdf:type owl:Class ;
                                               rdfs:comment "Set of axes that spans a given coordinate space and of mathematical rules for specifying how coordinates are to be assigned to points. Cf. ISO 19111:2007:2007-07, part 9.2, table 17 and annex B.2."@en ;
                                               rdfs:isDefinedBy "<http://data.ign.fr/def/ignf>" .


###  http://schema.org/Place
<http://schema.org/Place> rdf:type owl:Class ;
                          rdfs:comment "Entities that have a somewhat fixed, physical extension."@en ;
                          rdfs:seeAlso "http://schema.org/Place"^^xsd:anyURI .


###  http://sdm.hongo.wide.ad.jp/sdmo/Analyzer
<http://sdm.hongo.wide.ad.jp/sdmo/Analyzer> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Processor> ;
                                            rdfs:comment "A subclass of \"Processor\" that reads and processes media. It does not generate media."@en ,
                                                         "メディアを読み込んで処理を行うProcessor のサブクラス。メディア生成を行わない。"@ja ;
                                            rdfs:label "Analyzer"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/AudioMedia
<http://sdm.hongo.wide.ad.jp/sdmo/AudioMedia> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                              rdfs:comment "A subclass of \"Media\" for representing audio media."@en ,
                                                           "音声メディアを表現するための Media のサブクラス。"@ja ;
                                              rdfs:label "Audio Media"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/AudioPlayer
<http://sdm.hongo.wide.ad.jp/sdmo/AudioPlayer> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Player> ;
                                               rdfs:comment "A subclass of \"Player\" to represent a playback system for audio media."@en ,
                                                            "音声メディアの再生システムを表現するための Player のサブクラス。"@ja ;
                                               rdfs:label "Audio Player"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/AudioRecorder
<http://sdm.hongo.wide.ad.jp/sdmo/AudioRecorder> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Recorder> ;
                                                 rdfs:comment "A subclass of \"Recorder\" to represent a sound recording system that generates audio media."@en ,
                                                              "音声メディアを生成する音声収録システムを表現するための Recorder のサブクラス。"@ja ;
                                                 rdfs:label "Audio Recorder"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia
<http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Media> ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> ;
                                                                    owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                                  ] ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasMediaEvent> ;
                                                                    owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent>
                                                                  ] ;
                                                  rdfs:comment "A subclass of \"Media\" to represent media that integrate multiple media."@en ,
                                                               "複数のメディアを統合したメディアを表現するための Mediaのサブクラス。"@ja ;
                                                  rdfs:label "Composite Media"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/CompositePlayer
<http://sdm.hongo.wide.ad.jp/sdmo/CompositePlayer> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Player> ,
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> ;
                                                                     owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                   ] ;
                                                   rdfs:comment "A subclass of \"Player\" for integrating multiple media playback systems."@en ,
                                                                "複数のメディア再生システムを統合するための Player のサブクラス。"@ja ;
                                                   rdfs:label "Composite Player"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/CompositeProcessor
<http://sdm.hongo.wide.ad.jp/sdmo/CompositeProcessor> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Processor> ,
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> ;
                                                                        owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                      ] ;
                                                      rdfs:comment "A subclass of \"Processor\" to integrate multiple media processing systems."@en ,
                                                                   "複数のメディア処理システムを統合するための Processor のサブクラス。"@ja ;
                                                      rdfs:label "Composite Processor"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/CompositeRecorder
<http://sdm.hongo.wide.ad.jp/sdmo/CompositeRecorder> rdf:type owl:Class ;
                                                     rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Recorder> ,
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> ;
                                                                       owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                     ] ;
                                                     rdfs:comment "A subclass of \"Recorder\" for integrating multiple audio recording systems."@en ,
                                                                  "複数の音声収録システムを統合するための Recorder のサブクラス。"@ja ;
                                                     rdfs:label "Composite Recorder"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/Context
<http://sdm.hongo.wide.ad.jp/sdmo/Context> rdf:type owl:Class ;
                                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/playedBy> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/recordedBy> ;
                                                             owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://schema.org/name> ;
                                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onDataRange xsd:string
                                                           ] ;
                                           rdfs:comment "A class for unifying metadata to be added to \"Recorder,\" \"Processor,\" \"Player,\" \"Media,\" \"Context.\" One of the main classes of \"SDMO.\""@en ,
                                                        "Recorder,Processor,Player,Media,Contextに付加する MetaData を統一的に扱うためのクラス。SDMOの主要クラスの一つ。"@ja ;
                                           rdfs:label "Context"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/Converter
<http://sdm.hongo.wide.ad.jp/sdmo/Converter> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Processor> ;
                                             rdfs:comment "A subclass of \"Processor\" that outputs media after reading and processing it."@en ,
                                                          "メディアを読み込んで加工してメディアを出力する Processorのサブクラス。"@ja ;
                                             rdfs:label "Converter"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/CoordinateSystem
<http://sdm.hongo.wide.ad.jp/sdmo/CoordinateSystem> rdf:type owl:Class ;
                                                    rdfs:subClassOf <http://data.ign.fr/def/ignf#CoordinateSystem> ,
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/measures> ;
                                                                      owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Geometry>
                                                                    ] ;
                                                    rdfs:comment "A class for defining a coordinate system. It describes the meaning of the values in \"Geometry.\" It describes the geographic coordinate system of latitude and longitude, the spatial coordinate system defined in the event venue, the time axis of the recorded media file, etc."@en ,
                                                                 "座標系を定義するためのクラス。Geometryで表現した値の意味を規定する。緯度経度の地理座標系やイベント会場内に定義する空間座標系、収録したメディアファイルの時間軸などを記述する。"@ja ;
                                                    rdfs:label "Coordinate System"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/DataMedia
<http://sdm.hongo.wide.ad.jp/sdmo/DataMedia> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                             rdfs:comment "It's a subclass of \"Media\" that represents data except for audio and video."@en ,
                                                          "音声、映像以外のデータを表現するメディアに関する Mediaのサブクラス。"@ja ;
                                             rdfs:label "Data Media"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/DataPlayer
<http://sdm.hongo.wide.ad.jp/sdmo/DataPlayer> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Player> ;
                                              rdfs:comment "A subclass of \"Media\" to describe a data media playback system."@en ,
                                                           "データメディアの再生システムを表現するための Media のサブクラス。"@ja ;
                                              rdfs:label "Data Player"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/DataRecorder
<http://sdm.hongo.wide.ad.jp/sdmo/DataRecorder> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Recorder> ;
                                                rdfs:comment "A subclass of \"Recorder\" to represent a data media recording system."@en ,
                                                             "データメディアの収録システムを表現するための Recorder のサブクラス。"@ja ;
                                                rdfs:label "Data Recorder"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/Generator
<http://sdm.hongo.wide.ad.jp/sdmo/Generator> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://sdm.hongo.wide.ad.jp/sdmo/Processor> ;
                                             rdfs:comment "A subclass of the Processor that generates media. It does not have media input."@en ,
                                                          "メディアの生成を行うProcessorのサブクラス。メディア入力を持たない。"@ja ;
                                             rdfs:label "Generator"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/Geometry
<http://sdm.hongo.wide.ad.jp/sdmo/Geometry> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://data.ign.fr/def/geometrie#Point> ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/measuredBy> ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/CoordinateSystem>
                                                            ] ;
                                            rdfs:comment "CoordinateSystemで定義された座標系を用いた座標を記述するためのクラス。"@ja ,
                                                         "Describes coordinates using the coordinate system defined in \"CoordinateSystem.\""@en ;
                                            rdfs:label "Geometry"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/Media
<http://sdm.hongo.wide.ad.jp/sdmo/Media> rdf:type owl:Class ;
                                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/inputFrom> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/inputFrom> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isComponentOf> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isMediaOf> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/outputTo> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/outputTo> ;
                                                           owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/measuredBy> ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass <http://sdm.hongo.wide.ad.jp/sdmo/CoordinateSystem>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/endAt> ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:dateTimeStamp
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/mediaStartAt> ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:dateTimeStamp
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/startAt> ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:dateTimeStamp
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/contentUrl> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:anyURI
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/name> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onDataRange xsd:string
                                                         ] ;
                                         rdfs:comment "Represents a media file. One of the main classes of \"SDMO.\""@en ,
                                                      "メディアファイルを表現するためのクラス。SDMOの主要クラスの一つ。"@ja ;
                                         rdfs:label "Media"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent
<http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent> rdf:type owl:Class ;
                                              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/hasMedia> ;
                                                                owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                              ] ,
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://sdm.hongo.wide.ad.jp/sdmo/isMediaEventOf> ;
                                                                owl:someValuesFrom <http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia>
                                                              ] ,
