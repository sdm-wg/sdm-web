###  http://schema.org/contentLocation
<http://schema.org/contentLocation> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/SDMEvent> ;
                                    rdfs:range <http://schema.org/Place> ;
                                    rdfs:comment "The location depicted or described in the content. For example, the location in a photograph or painting."@en ;
                                    rdfs:seeAlso "http://schema.org/contentLocation"^^xsd:anyURI .


###  http://sdm.hongo.wide.ad.jp/sdmo/attendedBy
<http://sdm.hongo.wide.ad.jp/sdmo/attendedBy> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/attends> ;
                                              rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/SDMEvent> ;
                                              rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/Target> ;
                                              rdfs:comment "Refers to the \"SDMEvent\" that it attends to. The inverse of \"attends.\""@en ,
                                                           "自身が参加しているSDMEventを参照する。attendsと逆の意味。"@ja ;
                                              rdfs:label "attended by"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/attends
<http://sdm.hongo.wide.ad.jp/sdmo/attends> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Target> ;
                                           rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/SDMEvent> ;
                                           rdfs:comment "Represents the state in which itself is attending \"SDMEvent.\" The inverse link from the link is defined by \"attendedBy.\""@en ,
                                                        "自身がSDMEventに参加している状態を表す。attendedBy で逆方向リンクを表現できる。"@ja ;
                                           rdfs:label "attends"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/geometryAt
<http://sdm.hongo.wide.ad.jp/sdmo/geometryAt> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> ;
                                              rdfs:domain [ rdf:type owl:Class ;
                                                            owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                        )
                                                          ] ;
                                              rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/Geometry> ;
                                              rdfs:comment "Represents an object's position and time information by the \"Geometry\" linked by \"geometryAt.\" The inverse link from the linked \"Geometry\" is defined by \"geometryOf.\""@en ,
                                                           "geometryAt でリンクした Geometryでオブジェクトの位置・時間情報を表現する。リンク先のGeometryからの逆方向リンクはgeometryOfで表す。"@ja ;
                                              rdfs:label "geometry at"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/geometryOf
<http://sdm.hongo.wide.ad.jp/sdmo/geometryOf> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/Geometry> ;
                                              rdfs:range [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                                         <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                         <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                         <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                       )
                                                         ] ;
                                              rdfs:comment "The inverse link to the object linking itself with \"geometryAt.\""@en ,
                                                           "geometryAtで自身をリンクしているオブジェクトへの逆方向リンク。"@ja ;
                                              rdfs:label "geometry of"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/hasComponent
<http://sdm.hongo.wide.ad.jp/sdmo/hasComponent> rdf:type owl:ObjectProperty ;
                                                owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/isComponentOf> ;
                                                rdfs:domain [ rdf:type owl:Class ;
                                                              owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia>
                                                                            <http://sdm.hongo.wide.ad.jp/sdmo/CompositePlayer>
                                                                            <http://sdm.hongo.wide.ad.jp/sdmo/CompositeProcessor>
                                                                            <http://sdm.hongo.wide.ad.jp/sdmo/CompositeRecorder>
                                                                          )
                                                            ] ;
                                                rdfs:range [ rdf:type owl:Class ;
                                                             owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                                           <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                           <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                           <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                         )
                                                           ] ;
                                                rdfs:comment "CompositeRecorder,CompositeProcessor,CompositePlayer,CompositeMedia に包含されている Recorder,Processor, Player, Media をそれぞれ表現する。包含されたオブジェクトからはisComponentOf で被包含関係を表現する。"@ja ,
                                                             "Represents each \"Recorder,\" \"Processor,\" \"Player,\" \"Media\" that is included in the \"CompositeRecorder,\" \"CompositeProcessor,\" \"CompositePlayer,\" \"CompositeMedia.\" From the included objects, \"isComponentOf\" represents the included relationship."@en ;
                                                rdfs:label "has component"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/hasContext
<http://sdm.hongo.wide.ad.jp/sdmo/hasContext> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/isContextOf> ;
                                              rdfs:domain [ rdf:type owl:Class ;
                                                            owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Context>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                          <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                        )
                                                          ] ;
                                              rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/Context> ;
                                              rdfs:comment "Represents the \"Context\" attached to the object. The inverse link from the linked \"Context\" is defined by \"isContextOf.\""@en ,
                                                           "オブジェクトに付随する Context を表現する。リンク先の Context からは　isContextOf で逆方向リンクを表現できる。"@ja ;
                                              rdfs:label "has context"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/hasMedia
<http://sdm.hongo.wide.ad.jp/sdmo/hasMedia> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/isMediaOf> ;
                                            rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent> ;
                                            rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/Media> ;
                                            rdfs:comment "MediaEvent が参照する Media を表現する。参照先のMediaからは isMediaOf で逆方向リンクを表現できる。"@ja ,
                                                         "Represents the \"Media\" that is referenced by the \"MediaEvent.\" The inverse link from the linked \"Media\" is defined by \"isMediaOf.\""@en ;
                                            rdfs:label "has media"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/hasMediaEvent
<http://sdm.hongo.wide.ad.jp/sdmo/hasMediaEvent> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/isMediaEventOf> ;
                                                 rdfs:domain <http://sdm.hongo.wide.ad.jp/sdmo/CompositeMedia> ;
                                                 rdfs:range <http://sdm.hongo.wide.ad.jp/sdmo/MediaEvent> ;
                                                 rdfs:comment "CompositeMedia が包含している MediaEvent を表す。包含された MediaEvent からは　isMediaEventOf で包含元の CompositeMedia をリンクする。"@ja ,
                                                              "Represents the \"MediaEvent\" that is included by \"CompositeMedia.\" The inverse link from the linked \"MediaEvent\" is defined by \"isMediaEventOf.\""@en ;
                                                 rdfs:label "has media event"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/inputFrom
<http://sdm.hongo.wide.ad.jp/sdmo/inputFrom> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <http://sdm.hongo.wide.ad.jp/sdmo/outputTo> ;
                                             rdfs:domain [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                                         <http://sdm.hongo.wide.ad.jp/sdmo/Player>
                                                                         <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                       )
                                                         ] ;
                                             rdfs:range [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://sdm.hongo.wide.ad.jp/sdmo/Media>
                                                                        <http://sdm.hongo.wide.ad.jp/sdmo/Processor>
                                                                        <http://sdm.hongo.wide.ad.jp/sdmo/Recorder>
                                                                      )
                                                        ] ;
                                             rdfs:comment "A link to specify the input media. The link destination can be one of \"Recorder,\"  \"Processor,\" or \"Media.\" The inverse link from the linked media is defined by \"outputTo.\""@en ,
                                                          "入力となるメディアを指定するためのリンク。リンク先はRecorder , Processor , Media のいずれかになる。リンク先からはoutputTo により逆方向リンクを表現できる。"@ja ;
                                             rdfs:label "input from"@en .


###  http://sdm.hongo.wide.ad.jp/sdmo/isComponentOf
<http://sdm.hongo.wide.ad.jp/sdmo/isComponentOf> rdf:type owl:ObjectProperty ;
